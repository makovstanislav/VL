{"item":{
    "details":{
        "title":{
            "name":"Title",
            "description":"Specimen title.",
            "required":"TRUE",
            "type":"text",
            "lengthMax":60,
            "inputFormType":"text"
        },
        "description":{
            "name":"Description",
            "description":"Specimen short description.",
            "required":"TRUE",
            "type":"text",
            "lengthMax":1000,
            "inputFormType":"text"
        },
        "specimenType":{
            "name":"Type",
            "description":"Specimen type.",
            "required":"TRUE",
            "type":"keyValuePair",
            "option":{"tissue":"Tissue", "cells":"Cell line", "microbiome":"Microbiome"},
            "default":"tissue",
            "inputFormType":"radio"
        },
        "sourceOrganism":{
            "name":"Organism",
            "description":"Specimen source organism (NCBI taxon ID).",
            "required":"TRUE",
            "type":"keyValuePair",
            "choiceFrom":"OWL",
            "PURL":"http://purl.obolibrary.org/obo/ncbitaxon.owl",
            "default":"10090",
            "inputFormType":"radio"
        },
        "sourceStrain":{
            "name":"Strain",
            "description":"Specimen source strain. For mouse strains, use the Institute of Laboratory Animal Resources (ILAR) reference.",
            "required":"TRUE",
            "type":"text",
            "lengthMax":"60",
            "inputFormType":"text"
        },
        "sourceSamplingSite":{
            "name":"Sampling site",
            "description":"Sampling site (e.g. Liver).",
            "useIf":"source %in% ['tissue', 'microbiome']",
            "required":"TRUE",
            "type":"text",
            "lengthMax":"1000",
            "inputFormType":"text"
        },
        "samplingDate":{
            "name":"Sampling date",
            "description":"Sampling date.",
            "required":"TRUE",
            "type":"date",
            "inputFormType":"date"
        },
        "samplingTime":{
            "name":"Sampling time",
            "description":"Sampling time.",
            "type":"time",
            "inputFormType":"time"
        },
        "storageCondition":{
            "name":"Storage condition",
            "description":"Storage condition.",
            "required":"TRUE",
            "type":"keyValuePair",
            "option":{"-20":"-20°C", "-80":"-80°C", "4":"4°C", "cryogeny":"Cryogeny", "rt":"Room temperature"},
            "inputFormType":"radio"
        },
        "experimentalConditions":{
            "name":"Experimental conditions",
            "description":"A description of experimental conditions.",
            "type":"text",
            "lengthMax":"2000",
            "inputFormType":"text"
        },
        "treatment":{
            "name":"treatment",
            "description":"A description of treatment, if applicable.",
            "type":"text",
            "lengthMax":"2000",
            "inputFormType":"text"
        },
        "availableData":{
            "name":"Available data",
            "description":"A description of associated data, if applicable.",
            "type":"text",
            "lengthMax":"1000",
            "inputFormType":"text"
        }
    },
    "pricingAndAvailability": {
        "number":{
            "name":"Specimen number",
            "description":"Number of specimen.",
            "required":"TRUE",
            "type":"positiveInteger",
            "default":"1",
            "inputFormType":"number"
        },
        "volume":{
            "name":"Volume",
            "description":"Volume, if applicable.",
            "type":"positiveNumber1Digit",
            "inputFormType":"number"
        },
        "volumeUnit":{
            "name":"Volume unit",
            "description":"Volume unit, if applicable.",
            "type":"keyValuePair",
            "option":{"ul":"ul", "ml":"ml", "l":"l"},
            "inputFormType":"radio"
        },
        "concentration":{
            "name":"Concentration",
            "description":"Concentration, if applicable.",
            "type":"positiveNumber1Digit",
            "inputFormType":"number"
        },
        "concentrationUnit":{
            "name":"Concentration unit",
            "description":"Concentration unit, if applicable.",
            "type":"keyValuePair",
            "option":{"":"u/ul", "ml":"u/ml", "mm/l":"mm/ml"},
            "inputFormType":"radio"
        },
        "availability":{
            "name":"Availability",
            "description":"Availability.",
            "required":"TRUE",
            "type":"keyValuePair",
            "option":{"inStock":"In stock", "onDemand":"On demand"},
            "inputFormType":"checkbox"
        },
        "price":{
            "name":"Price",
            "description":"Price per item.",
            "required":"TRUE",
            "type":"positiveNumber2Digits",
            "inputFormType":"number"
        },
        "priceCurrency":{
            "name":"Currency",
            "description":"Currency.",
            "required":"TRUE",
            "type":"keyValuePair",
            "option":{"CHF":"CHF", "USD":"USD", "EUR":"EUR"},
            "inputFormType":"radio"
        }
    },
